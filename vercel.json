{
  "rewrites": [
    {
      "source": "/teachers-training",
      "destination": "/teachers-training.html"
    },
    {
      "source": "/postpartum-fitness",
      "destination": "/postpartum-fitness.html"
    },
    {
      "source": "/postpartum",
      "destination": "/postpartum-fitness.html"
    },
    {
      "source": "/about",
      "destination": "/about.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "status": 404, "dest": "/api/404.js" }
  ],
  "headers": [
    {
      "source": "/teachers-training.html",
      "headers": [
        {
          "key": "Link",
          "value": "</teachers-training>; rel=\"canonical\""
        }
      ]
    },
    {
      "source": "/postpartum-fitness.html",
      "headers": [
        {
          "key": "Link",
          "value": "</postpartum-fitness>; rel=\"canonical\""
        }
      ]
    },
    {
      "source": "/about.html",
      "headers": [
        {
          "key": "Link",
          "value": "</about>; rel=\"canonical\""
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        }
      ]
    }
  ]
}
